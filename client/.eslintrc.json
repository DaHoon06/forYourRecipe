{
  "root": true,
  "extends": [
    "react-app",
    "react-app/jest",
    "prettier",
    "plugin:react/recommended"
  ],
  "plugins": [
    "unused-imports"
  ],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "off",
    //    "unused-imports/no-unused-imports": "off",
    "unused-imports/no-unused-vars": [
      "off"
    ],
    "react/react-in-jsx-scope": "off"
  }
}
